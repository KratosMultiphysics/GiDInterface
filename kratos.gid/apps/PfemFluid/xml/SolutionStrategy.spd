<?xml version="1.0" encoding="utf-8"?>
<container n="SolStrat" pn="Solver Settings" un="PFEMFLUID_StratSection" icon="linear_solver" help="Set linear and non-linear solvers" open_window="0">
    <container n="AnalysisType" pn="Analysis type" icon="select" un="PFEMFLUID_AnalysisType" help="Choose the solution strategy" actualize_tree="1" state="normal" tree_state="open" open_window="0">
    <value n="SolutionType" pn="Solution type" icon="data" un="PFEMFLUID_SolutionType" v="Dynamic" values="Static,Dynamic" help="Solution type" actualize_tree="1" state="[SolutionTypeState]"/>
    <value n="SolStratCombo" pn="Solving strategy" icon="data" un="PFEMFLUID_SolStrat" v="" help= "Select non-linear solver" values="" dict="[GetSolutionStrategiesPFEM]" actualize_tree="1" state="[DisableIfUniqueName PFEMFLUID_SolutionType Static]"/>
    <value n="Scheme" pn="Scheme" icon="data" un="PFEMFLUID_Scheme" v="" values="" dict="[GetSchemes]" state="hidden"  actualize_tree="1"/>
    <!-- <value n="LinearType" pn="Linear type" un="PFEMFLUID_LinearType" v="Linear" values="Linear,Non-Linear" help="Linear type" state="[checkStateByUniqueName PFEMFLUID_SolutionType Static]"/> -->
  </container> 
  <container n="StratParams" pn="Non-linear solver settings" help="Set non-linear solver parameters" un="PFEMFLUID_StratParams" icon="select"  state="normal" open_window="0">
    <dynamicnode command="spdAux::injectSolStratParams" args=""/>
  </container>
  <dynamicnode command="spdAux::injectSolvers" args=""/>
</container>
