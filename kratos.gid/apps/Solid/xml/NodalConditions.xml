<?xml version="1.0" encoding="UTF-8"?>
<NodalConditionList>
  <NodalConditionItem n="DISPLACEMENT" pn="Displacement" ProcessName="AssignVectorComponentsToNodesProcess" VariableName="DISPLACEMENT" ImplementedInApplication="SolidMechanicsApplication"
		      analysis_type="Static,Dynamic"  unit_magnitude="L" units="m" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
    <outputs>
      <parameter n="REACTION" pn="Reaction" v="Yes"/>
    </outputs>
  </NodalConditionItem>
  
  <NodalConditionItem n="VELOCITY" pn="Velocity" ProcessName="AssignVectorComponentsToNodesProcess" VariableName="VELOCITY" ImplementedInApplication="SolidMechanicsApplication"
		      analysis_type="Dynamic" unit_magnitude="Velocity" units="m/s" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>
  
  <NodalConditionItem  n="ACCELERATION" pn="Acceleration" ProcessName="AssignVectorComponentsToNodesProcess" VariableName="ACCELERATION" ImplementedInApplication="SolidMechanicsApplication"
		       analysis_type="Dynamic" unit_magnitude="Acceleration" units="m/s^2" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>
  
  <NodalConditionItem  n="PRESSURE" pn="Pressure" ProcessName="AssignScalarToNodesProcess" VariableName="PRESSURE" ImplementedInApplication="SolidMechanicsApplication"
		       analysis_type="Static,Dynamic"  unit_magnitude="P"  units="Pa" v="Yes" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>
  
  <NodalConditionItem  n="RIGID_BODY_ROTATION" pn="Rotation" ProcessName="ApplyRigidBodyRotationProcess" VariableName="DISPLACEMENT" ImplementedInApplication="SolidMechanicsApplication"
		       analysis_type="Static" unit_magnitude="Angle" units="rad" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>
  
  <NodalConditionItem  n="RIGID_BODY_ANGULAR_VELOCITY" pn="Angular Velocity" ProcessName="ApplyRigidBodyRotationProcess" VariableName="VELOCITY" ImplementedInApplication="SolidMechanicsApplication"
		       analysis_type="Dynamic" unit_magnitude="Angle/T" units="rad/s" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>
  
  <NodalConditionItem  n="RIGID_BODY_ANGULAR_ACCELERATION" pn="Angular Acceleration" ProcessName="ApplyRigidBodyRotationProcess" VariableName="ACCELERATION" ImplementedInApplication="SolidMechanicsApplication"
		       analysis_type="Dynamic" unit_magnitude="Angle/T^2" units="rad/s^2" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>

   <NodalConditionItem n="ROTATION" pn="Rotation" ProcessName="AssignVectorComponentsToNodesProcess" App="Structural" ImplementedInApplication="SolidMechanicsApplication"
			analysis_type="Static,Dynamic" unit_magnitude="Angle" units="rad" VariableName="ROTATION" state="CheckNodalConditionStateSolid" Interval="Total">
    <outputs>
      <parameter n="TORQUE" pn="Torque" v="Yes"/>
    </outputs>
   </NodalConditionItem>
   
  <NodalConditionItem n="ANGULAR_VELOCITY" pn="Angular Velocity" ProcessName="AssignVectorComponentsToNodesProcess" state="CheckNodalConditionStateSolid" Interval="Total"
			analysis_type="Dynamic" unit_magnitude="Angle/T" units="rad/s" VariableName="ANGULAR_VELOCITY" App="Structural" ImplementedInApplication="SolidMechanicsApplication">
  </NodalConditionItem>
  
  <NodalConditionItem n="ANGULAR_ACCELERATION" pn="Angular Acceleration" ProcessName="AssignVectorComponentsToNodesProcess" state="CheckNodalConditionStateSolid" Interval="Total"
			 analysis_type="Dynamic" unit_magnitude="Angle/T^2" units="rad/s^2"  VariableName="ANGULAR_ACCELERATION" App="Structural" ImplementedInApplication="SolidMechanicsApplication">
  </NodalConditionItem>  
</NodalConditionList>
