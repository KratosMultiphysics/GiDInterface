          <parameter n="AngularStartTime" parent="LinearPeriodic" pn="Angular start time" type="double" v="0.0" units="s" unit_magnitude="T" show_in_window="0"/>
          <parameter n="AngularEndTime" parent="LinearPeriodic" pn="Angular end time" type="double" v="1000.0" units="s" unit_magnitude="T" show_in_window="0"/>

          <parameter n="Ax" parent="FixedDOFs" type="combo" v="No" pn="Fix Velocity X" values="No,Constant" pvalues="No,Constant" help="Fix Velocity X" show_in_window="0">
            <parameter n="Vx" parent="Constant" type="double" v="1.0" pn="   Value" help="X Velocity" show_in_window="0"/>
          </parameter>
          <parameter n="Ay" parent="FixedDOFs" type="combo" v="No" pn="Fix Velocity Y" values="No,Constant" pvalues="No,Constant" help="Fix Velocity Y" show_in_window="0">
            <parameter n="Vy" parent="Constant" type="double" v="0.0" pn="   Value" help="Y Velocity" show_in_window="0"/>
          </parameter>
          <parameter n="Az" parent="FixedDOFs" type="combo" v="No" pn="Fix Velocity Z" values="No,Constant" pvalues="No,Constant" help="Fix Velocity Z" show_in_window="0">
            <parameter n="Vz" parent="Constant" type="double" v="0.0" pn="   Value" help="Z Velocity" show_in_window="0"/>
          </parameter>
          <parameter n="Bx" parent="FixedDOFs" type="combo" v="No" pn="Fix Angular Velocity X" values="No,Constant" pvalues="No,Constant" help="Fix Angular Velocity X" show_in_window="0">
            <parameter n="AVx" parent="Constant" type="double" v="0.0" pn="   Value" unit_magnitude="Angle/T" units="rad/s" help="X Angular Velocity" show_in_window="0"/>
          </parameter>
          <parameter n="By" parent="FixedDOFs" type="combo" v="No" pn="Fix Angular Velocity Y" values="No,Constant" pvalues="No,Constant" help="Fix Angular Velocity Y" show_in_window="0">
            <parameter n="AVy" parent="Constant" type="double" v="0.0" pn="   Value" unit_magnitude="Angle/T" units="rad/s" help="Y Angular Velocity" show_in_window="0"/>
          </parameter>
          <parameter n="Bz" parent="FixedDOFs" type="combo" v="No" pn="Fix Angular Velocity Z" values="No,Constant" pvalues="No,Constant" help="Fix Angular Velocity Z" show_in_window="0">
            <parameter n="AVz" parent="Constant" type="double" v="0.0" pn="   Value" unit_magnitude="Angle/T" units="rad/s" help="Z Angular Velocity" show_in_window="0"/>
          </parameter>
          <parameter n="VStart" parent="FixedDOFs" type="double" v="0.0" pn="Start time" show_in_window="0"/>
          <parameter n="VEnd" parent="FixedDOFs" type="double" v="100.0" pn="End time" show_in_window="0"/>

