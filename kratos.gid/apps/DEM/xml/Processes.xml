<?xml version="1.0" encoding="UTF-8"?>
<ProcessList>
  <Process n="DEM-FEM-Wall-Process" pn="Impose vector value by direction process" python_module="assign_vector_by_direction_to_condition_process" kratos_module="KratosMultiphysics" help="This process fixes all the components of a given vector variable" check="DirectorVectorNonZero direction">
    <inputs>
      <parameter n="fixed_wall" pn="Update velocity, not displacements" type="bool" v="No" values="Yes,No" help="If activated, the wall will not move and will just have an imposed velocity field"/>
      <parameter n="friction_coeff" pn="Friction angle (deg)" type="double" v="30.0" help="Coulomb friction coefficient" />
      <parameter n="CohesiveWall" pn="Cohesive wall" type="bool" v="No" values="Yes,No" help="Specify if the wall presents a cohesive behaviour" />
      <parameter n="DEM_Wear" pn="Compute Wear" type="bool" v="No" values="Yes,No" help="If activated, wear will be computed on this wall. It can increase computation time" />
      <parameter n="K_Abrasion" pn="Severity of Abrasion" type="double" v="0.001" help="Archards abrasion K parameter" />
      <parameter n="K_Impact" pn="Severity of Impact" type="double" v="0.001" help="Archards impact K parameter" />
      <parameter n="H_Brinell" pn="Brinell Hardness (HB)" type="double" v="200.0" help="Brinell hardness value" />
      <parameter n="RigidPlane" pn="Rigid Structure" type="bool" v="No" values="Yes,No" help="Give this DEM-FEM group an ideally rigid behaviour or an elastic response with a particular Young Modulus" />
      <parameter n="YoungModulus" pn="Young Modulus (Pa)" type="double" v="1e20" />
      <parameter n="PoissonRatio" pn="Poisson Ratio" type="double" v="0.25" />
      <parameter n="PrescribeMotion" pn="Prescribe motion" type="bool" v="0">
        <parameter n="LINEAR_VELOCITYX" pn="Vx" type="double" v="0.0" parent="1" unit_magnitude="Velocity" units="m/s"/>
        <parameter n="LINEAR_VELOCITYY" pn="Vy" type="double" v="0.0" parent="1" unit_magnitude="Velocity" units="m/s"/>
        <parameter n="LINEAR_VELOCITYZ" pn="Vz" type="double" v="0.0" parent="1" unit_magnitude="Velocity" units="m/s"/>
        <parameter n="LINEAR_VELOCITY_PERIODIC" pn="Periodic linear velocity" type="bool" parent="1" v="0">
          <parameter n="LINEAR_VELOCITY_PERIOD" pn="Period linear velocity" parent="1" type="double" v="1.0" units="s" unit_magnitude="T"/></parameter>
        <parameter n="ANGULAR_VELOCITYX" pn="ω x" type="double" v="0.0" function="0" parent="1" unit_magnitude="Angle/T" units="rad/s"/>
        <parameter n="ANGULAR_VELOCITYY" pn="ω y" type="double" v="0.0" function="0" parent="1" unit_magnitude="Angle/T" units="rad/s"/>
        <parameter n="ANGULAR_VELOCITYZ" pn="ω z" type="double" v="0.0" function="0" parent="1" unit_magnitude="Angle/T" units="rad/s"/>
        <parameter n="ROTATION_CENTERX" pn="Center x" type="double" v="0.0" parent="1" />
        <parameter n="ROTATION_CENTERY" pn="Center y" type="double" v="0.0" parent="1" />
        <parameter n="ROTATION_CENTERZ" pn="Center z" type="double" v="0.0" parent="1" />
        <parameter n="ANGULAR_VELOCITY_PERIODIC" pn="Periodic angular velocity" parent="1" type="bool" v="0">
          <parameter n="ANGULAR_VELOCITY_PERIOD" pn="Period angular velocity" parent="1" type="double" v="1.0" units="s" unit_magnitude="T"/></parameter>
      </parameter>
    </inputs>
  </Process>
  <Process n="DEMInletProcess" pn="" python_module="" kratos_module="KratosMultiphysics" help="">
    <inputs>
      <parameter n="ELEMENT_TYPE" pn="Element type" type="combo" v="Sphere" values="Sphere,Cluster"/>
      <parameter n="VELOCITY_MODULUS" pn="Velocity modulus" type="double" v="1.0" units="m/s" unit_magnitude="Velocity"/>
      <parameter n="DIRECTION_VECTOR" pn="Direction vector" type="vector" v="1.0,0.0,0.0" />
      <parameter n="RANDOM_VELOCITY_DEVIATION" pn="Max. random velocity deviation" type="double" v="1.0" units="deg" unit_magnitude="Angle"/>
      <parameter n="FLOW_MEASUREMENT" pn="Flow measurement" type="combo" v="Particles" values="Particles,Kilograms" pvalues="Particles per second,Kilograms per second">
        <parameter n="NUMBER_OF_PARTICLES" pn="Particles per second" parent="Particles" type="double" v="1.0" />
        <parameter n="NUMBER_OF_KILOGRAMS" pn="Kilograms per second" parent="Kilograms" type="double" v="1.0" unit_magnitude="M/T" units="kg/s" />
      </parameter>
      <parameter n="DIAMETER" pn="Diameter" type="double" v="1.0" units="m" unit_magnitude="L"/>
      <parameter n="PROBABILITY_DISTRIBUTION" pn="Probability distribution" type="combo" v="Normal" values="Normal,Lognormal"/>
      <parameter n="STANDARD_DEVIATION" pn="Standard deviation" type="double" v="0.0" />
    </inputs>
  </Process>
  <Process n="Angular_Velocity_Process" pn="" python_module="" kratos_module="KratosMultiphysics" help="">
    <inputs>
      <parameter n="ANGULAR_VELOCITY" pn="ω" type="vector" v="0.0,0.0,0.0" function="0" has_units="1"/>
      <parameter n="ROTATION_CENTER" pn="Center" type="vector" v="0.0,0.0,0.0" />
      <parameter n="ANGULAR_VELOCITY_PERIODIC" pn="Periodic angular velocity" type="bool" v="0">
        <parameter n="ANGULAR_VELOCITY_PERIOD" pn="Period angular velocity" parent="1" type="double" v="1.0" units="s" unit_magnitude="T"/></parameter>
    </inputs>
  </Process>
  <Process n="Linear_Velocity_Process" pn="" python_module="" kratos_module="KratosMultiphysics" help="">
    <inputs>
      <parameter n="LINEAR_VELOCITY" pn="V" type="vector" v="0.0,0.0,0.0" function="0" has_units="1"/>
      <parameter n="LINEAR_VELOCITY_PERIODIC" pn="Periodic linear velocity" type="bool" v="0">
        <parameter n="LINEAR_VELOCITY_PERIOD" pn="Period linear velocity" parent="1" type="double" v="1.0" units="s" unit_magnitude="T"/></parameter>
    </inputs>
  </Process>
</ProcessList>