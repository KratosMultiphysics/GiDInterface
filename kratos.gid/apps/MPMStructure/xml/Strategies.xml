		<linearSolvers>
			<linearSolverItem n="coupling_strategy" pn="Coupling strategy" default="MVQN">
				<filterFeatures>
					<filter field="SolverType" value="MPMStructureCoupling"/>
				</filterFeatures>
				<defaults></defaults>
			</linearSolverItem>
		</linearSolvers>
