			<scheme n="DirichletNeumann" pn="Dirichlet-Neumann" help="" NeedElements="false">
				<parameter_list></parameter_list>
				<element_filters>
					<filter field="ImplementedInApplication" value="MPMStructureApplication"/>
				</element_filters>
			</scheme>
		</schemes>
	</StrategyItem>
