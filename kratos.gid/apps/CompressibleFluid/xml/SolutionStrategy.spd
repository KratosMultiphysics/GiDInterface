<?xml version="1.0" encoding="utf-8"?>
<container n="SolutionStrat" pn="Solution" un="CFSolutionParameters,FLSolutionParameters" icon="solution" help="Solution Strategy" open_window="0">
	<include n="Parallelism" active="1" path="apps/Common/xml/Parallelism.spd"/>
	<dynamicnode command="spdAux::injectSolvers" args=""/>
	<container n="StratParams" pn="Strategy parameters" un="CFStratParams,FLStratParams" icon="groupsTreeNew.gif" state="normal" open_window="0">
		<dynamicnode command="spdAux::injectSolStratParams" args=""/>
	</container>
	<container n="Gravity" pn="Gravity" un="CFGravity,FLGravity" help="Body forces" open_window="0" icon="gravity">
		<value n="GravityValue" pn="Gravity value" v="0.0" help="Gravity value" unit_magnitude="Acceleration" units="m/s^2" />
		<value n="Cx" pn="Cx" v="0.0" help="X Vector"/>
		<value n="Cy" pn="Cy" v="-1.0" help="Y Vector"/>
		<value n="Cz" pn="Cz" v="0.0" help="Z Vector" state="[CheckDimension 3D]"/>
	</container>
	<container n="TimeParameters" pn="Time parameters" un="CFTimeParameters,FLTimeParameters" help="Time parameters" open_window="0" icon="time4">
		<value n="StartTime" pn="Start time" v="0.0" help=""/>
		<value n="EndTime" pn="End time" v="1.0" help=""/>
		<value n="AutomaticDeltaTime" pn="Automatic delta time" un="CFAutomaticDeltaTime,FLAutomaticDeltaTime" v="No" values="Yes,No" help=""/>
		<value n="DeltaTime"              pn="Delta time"                     v="0.01" help="User-defined time step"                  state="[checkStateByUniqueName CFAutomaticDeltaTime No]"/>
		<value n="CFL_Number"             pn="Maximum CFL number"             v="1.0"  help="Maximum Courant–Friedrichs–Lewy allowed" state="[checkStateByUniqueName CFAutomaticDeltaTime Yes]"/>
		<value n="Viscous_Fourier_number" pn="Maximum viscous Fourier number" v="1.0"  help="Maximum viscous Fourier number allowed"  state="[checkStateByUniqueName CFAutomaticDeltaTime Yes]"/>
		<value n="Thermal_Fourier_number" pn="Maximum thermal Fourier number" v="1.0"  help="Maximum thermal Fourier number allowed"  state="[checkStateByUniqueName CFAutomaticDeltaTime Yes]"/>
		<value n="MinimumDeltaTime"       pn="Minimum delta time"             v="1e-8" help="Minimum delta time allowed"              state="[checkStateByUniqueName CFAutomaticDeltaTime Yes]"/>
		<value n="MaximumDeltaTime"       pn="Maximum delta time"             v="1e-3" help="Maximum delta time allowed"              state="[checkStateByUniqueName CFAutomaticDeltaTime Yes]"/>
	</container>
</container>
