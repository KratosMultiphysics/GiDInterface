            <parameter n="CONDUCTIVITY"        pn="Conductivity"        unit_magnitue="M*L/T^3/Temp" units="W/m/K"    v="0.024"  help="Set the Conductivity"        />
            <parameter n="SPECIFIC_HEAT"       pn="Specific_heat"       unit_magnitue="L^2/Temp/T^2" units="J/K/kg"   v="722.14" help="Set the Specific_heat"       />
            <parameter n="HEAT_CAPACITY_RATIO" pn="Heat capacity ratio" unit_magnitue=""             units=""         v="1.4"    help="Set the Heat capacity ratio" />
        </inputs>
        <outputs>
        </outputs>
    </CLaw>

</ConstitutiveLaws>
