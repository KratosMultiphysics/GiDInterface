      <!-- help=""/>-->
      <parameter n="time_step_prediction_level" pn="Time step prediction level" type="double" v="0" values="0,1,2" help="Prediction level possibilities [0,1,2]"/>
      <parameter n="rayleigh_damping" pn="Rayleigh damping" type="bool" v="false" values="false,true" help="Switch on/off the Rayleigh damping"/>
    </parameter_list>

    <linearSolvers>
      <linearSolverItem n="linear_solver_settings" pn="Linear Solver">
        <filterFeatures>
          <filter field="SolverType" value="Standard"/>
        </filterFeatures>
        <defaults>
          <solver n="Automatic">
          </solver>
        </defaults>
      </linearSolverItem>
    </linearSolvers>

    <schemes>
      <scheme n="CentralDifferences" pn="Central Differences" ProductionReady="ProductionReady">
        <parameter_list>
          <!--<parameter n="Beta" pn="Beta" type="double" v="0.25" help="T"/>
	    <parameter n="Gamma" pn="Gamma" type="double" v="0.5" help="T"/> -->
        </parameter_list>
        <element_filters>
          <filter field="kratos_module" value="SolidMechanicsApplication,PfemSolidMechanicsApplication,ContactMechanicsApplication"/>
        </element_filters>
      </scheme>
    </schemes>

  </StrategyItem>
  <StrategyItem n="Static" pn="Implicit" SolutionType="Static" EquationType="Monolithic" kratos_module="SolidMechanicsApplication" python_module="solid_mechanics_static_solver" MinimumKratosVersion="8500" App="Solid" ProductionReady="ProductionReady" help="Default solver Super LU">

    <!-- list of options to be filled -->
    <parameter_list>
      <parameter n="line_search" pn="Line search" type="bool" v="false" values="false,true" help="Switch on/off line search"/>
      <parameter n="convergence_criterion" pn="Convergence criterion" type="combo" v="Residual_criterion"
		 values ="Residual_criterion,Variable_criterion,And_criterion,Or_criterion"
		 pvalues ="Residual criterion,Variable criterion,And criterion,Or criterion" help="These are the available convergence criteria for the nonlinear iteration">
        <parameter n="variable_relative_tolerance" pn="Variable relative tolerance" type="double" v="1e-4" parent="Variable_criterion,And_criterion,Or_criterion" help="Relative displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="variable_absolute_tolerance" pn="Variable absolute tolerance" type="double" v="1e-9" parent="Variable_criterion,And_criterion,Or_criterion" help="Absolute displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_relative_tolerance" pn="Residual relative tolerance" type="double" v="1e-4" parent="Residual_criterion,And_criterion,Or_criterion" help="Relative residual tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_absolute_tolerance" pn="Residual absolute tolerance" type="double" v="1e-9" parent="Residual_criterion,And_criterion,Or_criterion" help="Absolute residual tolerance employed in the nonlinear iteration"/>
      </parameter>
      <parameter n="max_iteration" pn="Maximum iterations" type="integer" v="10" help="This number will break the iterative loop when reached"/>
    </parameter_list>

    <linearSolvers>
      <linearSolverItem n="linear_solver_settings" pn="Linear Solver">
        <filterFeatures>
          <filter field="SolverType" value="Standard"/>
        </filterFeatures>
        <defaults>
          <solver n="Automatic">
          </solver>
        </defaults>
      </linearSolverItem>
    </linearSolvers>

    <schemes>
      <scheme n="Static" pn="Static" ProductionReady="ProductionReady">
        <parameter_list></parameter_list>
        <element_filters>
          <filter field="kratos_module" value="SolidMechanicsApplication,PfemSolidMechanicsApplication,ContactMechanicsApplication"/>
        </element_filters>
      </scheme>
    </schemes>

  </StrategyItem>
  <StrategyItem n="Quasi-static" pn="Implicit" SolutionType="Quasi-static" EquationType="Monolithic" kratos_module="SolidMechanicsApplication" python_module="solid_mechanics_static_solver" MinimumKratosVersion="8500" App="Solid" ProductionReady="ProductionReady" help="Default solver Super LU">

    <!-- list of options to be filled -->
    <parameter_list>
      <parameter n="line_search" pn="Line search" type="bool" v="false" values="false,true" help="Switch on/off line search"/>
      <parameter n="convergence_criterion" pn="Convergence criterion" type="combo" v="Residual_criterion" 
        values ="Residual_criterion,Variable_criterion,And_criterion,Or_criterion" pvalues ="Residual criterion,Variable criterion,And criterion,Or criterion" help="These are the available convergence criteria for the nonlinear iteration">
        <parameter n="variable_relative_tolerance" pn="Variable relative tolerance" type="double" v="1e-4" parent="Variable_criterion,And_criterion,Or_criterion" help="Relative displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="variable_absolute_tolerance" pn="Variable absolute tolerance" type="double" v="1e-9" parent="Variable_criterion,And_criterion,Or_criterion" help="Absolute displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_relative_tolerance" pn="Residual relative tolerance" type="double" v="1e-4" parent="Residual_criterion,And_criterion,Or_criterion" help="Relative residual tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_absolute_tolerance" pn="Residual absolute tolerance" type="double" v="1e-9" parent="Residual_criterion,And_criterion,Or_criterion" help="Absolute residual tolerance employed in the nonlinear iteration"/>
      </parameter>
      <parameter n="max_iteration" pn="Maximum iterations" type="integer" v="10" help="This number will break the iterative loop when reached"/>
    </parameter_list>

    <linearSolvers>
      <linearSolverItem n="linear_solver_settings" pn="Linear Solver">
        <filterFeatures>
          <filter field="SolverType" value="Standard"/>
        </filterFeatures>
        <defaults>
          <solver n="Automatic">
          </solver>
        </defaults>
      </linearSolverItem>
    </linearSolvers>

    <schemes>
      <scheme n="Static" pn="Static" ProductionReady="ProductionReady">
        <parameter_list></parameter_list>
        <element_filters>
          <filter field="kratos_module" value="SolidMechanicsApplication"/>
          <filter field="kratos_module" value="SolidMechanicsApplication,PfemSolidMechanicsApplication,ContactMechanicsApplication"/>
        </element_filters>
      </scheme>
    </schemes>

  </StrategyItem>
  <StrategyItem n="SegregatedVPStrategy" pn="Implicit" SolutionType="Dynamic" EquationType="Segregated" kratos_module="PfemApplication" python_module="solid_mechanics_segregated_solver" MinimumKratosVersion="9000" App="Pfem" ProductionReady="No" help="">

    <!-- list of options to be filled -->
    <parameter_list>
      <parameter n="convergence_criterion" pn="Convergence criterion" type="combo" v="Variable_criterion"
	       values ="Residual_criterion,Variable_criterion,And_criterion,Or_criterion"
	       pvalues ="Residual criterion,Variable criterion,And criterion,Or criterion" help="These are the available convergence criteria for the nonlinear iteration">
        <parameter n="variable_relative_tolerance" pn="Variable relative tolerance" type="double" v="1e-5" parent="Variable_criterion,And_criterion,Or_criterion" help="Relative displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="variable_absolute_tolerance" pn="Variable absolute tolerance" type="double" v="1e-9" parent="Variable_criterion,And_criterion,Or_criterion" help="Absolute displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_relative_tolerance" pn="Residual relative tolerance" type="double" v="1e-5" parent="Residual_criterion,And_criterion,Or_criterion" help="Relative residual tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_absolute_tolerance" pn="Residual absolute tolerance" type="double" v="1e-9" parent="Residual_criterion,And_criterion,Or_criterion" help="Absolute residual tolerance employed in the nonlinear iteration"/>
      </parameter>
      <parameter n="max_iteration" pn="Maximum iterations" type="integer" v="20" help="This number will break the iterative loop when reached"/>
    </parameter_list>

    <linearSolvers>
      <linearSolverItem n="linear_solver_settings" pn="Linear Solver" help="Default solver Super LU">
        <filterFeatures>
          <filter field="SolverType" value="Standard"/>
        </filterFeatures>
        <defaults>
          <solver n="bicgstab">
          </solver>
        </defaults>
      </linearSolverItem>
    </linearSolvers>

    <schemes>
      <scheme n="Newmark" pn="Newmark" help="Scheme for Solids and Fluids, dynamic problems" ProductionReady="ProductionReady">
        <parameter_list>
          <parameter n="lumped_mass_matrix" pn="Lumped mass matrix" type="bool" v="true" values="false,true" help="Compute lumped mass matrix"/>
          <parameter n="consistent_mass_matrix" pn="Consistent mass matrix" type="bool" v="false" values="false,true" help="Compute consistent mass matrix"/>
          <!--<parameter n="Beta" pn="Beta" type="double" v="0.25" help="T"/> -->
          <!--<parameter n="Gamma" pn="Gamma" type="double" v="0.5" help="T"/> -->
        </parameter_list>
        <element_filters>
          <filter field="Dofs" value="VELOCITY"/>
          <forceIn field="ElementType" value="Rigid"/>
          <!--<filter field="ElementType" value="Solid,Fluid,Rigid"/> -->
          <!--<filter field="kratos_module" value="PfemApplication,SolidMechanicsApplication,ContactMechanicsApplication"/> -->
        </element_filters>
      </scheme>
      <scheme n="Bossak" pn="Bossak" help="Scheme for Solids and Fluids, dynamic problems with high frequency accelerations damping" ProductionReady="ProductionReady">
        <parameter_list>
          <parameter n="bossak_factor" pn="Bossak factor" type="double" v="-0.3" help="Damping factor for the bossak scheme [-0.3,0]"/>
          <parameter n="lumped_mass_matrix" pn="Lumped mass matrix" type="bool" v="true" values="false,true" help="Compute lumped mass matrix"/>
          <parameter n="consistent_mass_matrix" pn="Consistent mass matrix" type="bool" v="false" values="false,true" help="Compute consistent mass matrix"/>
        </parameter_list>
        <element_filters>
          <filter field="Dofs" value="VELOCITY"/>
          <forceIn field="ElementType" value="Rigid"/>
        </element_filters>
      </scheme>
      <scheme n="BackwardEuler" pn="Backward Euler" help="Scheme for Solids and Fluids, dynamic problems" ProductionReady="Developer">
        <parameter_list>
          <parameter n="time_integration_order" pn="Time order" type="combo" values="1,2,3,4,5,6" v="2" help="Time integration order" state="hidden"/>
          <parameter n="lumped_mass_matrix" pn="Lumped mass matrix" type="bool" v="true" values="false,true" help="Compute lumped mass matrix"/>
          <parameter n="consistent_mass_matrix" pn="Consistent mass matrix" type="bool" v="false" values="false,true" help="Compute consistent mass matrix"/>
        </parameter_list>
        <element_filters>
          <filter field="Dofs" value="VELOCITY"/>
          <forceIn field="ElementType" value="Rigid"/>
        </element_filters>
      </scheme>
      <scheme n="Bdf" pn="BDF" help="Scheme for Solids and Fluids, dynamic problems" ProductionReady="Developer">
        <parameter_list>
          <parameter n="time_integration_order" pn="Time order" type="combo" values="1,2,3,4,5,6" v="1" help="Time integration order"/>
          <parameter n="lumped_mass_matrix" pn="Lumped mass matrix" type="bool" v="true" values="false,true" help="Compute lumped mass matrix"/>
          <parameter n="consistent_mass_matrix" pn="Consistent mass matrix" type="bool" v="false" values="false,true" help="Compute consistent mass matrix"/>
        </parameter_list>
        <element_filters>
          <filter field="Dofs" value="VELOCITY"/>
          <forceIn field="ElementType" value="Rigid"/>
        </element_filters>
      </scheme>
    </schemes>

  </StrategyItem>
</StrategyList>
