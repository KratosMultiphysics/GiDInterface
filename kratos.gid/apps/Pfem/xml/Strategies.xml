<?xml version="1.0" encoding="UTF-8"?>
<StrategyList>
  <StrategyItem n="Implicit" pn="Implicit" SolutionType="Dynamic" EquationType="Monolithic" kratos_module="SolidMechanicsApplication" python_module="solid_mechanics_implicit_dynamic_solver" MinimumKratosVersion="8500" App="Solid" ProductionReady="ProductionReady" help="">

    <!-- list of options to be filled -->
    <parameter_list>
      <parameter n="line_search" pn="Line search" type="bool" v="false" values="false,true" help="Switch on/off line search"/>
      <!--<parameter n="reform_dofs_at_each_step" pn="Reform DOFs at each iteration" type="bool" v="false" values="false,true" help=""/> -->
      <!--<parameter n="compute_reactions" pn="Compute reactions" type="bool" v="true" values="true,false" help=""/> -->
      <!--<parameter n="block_builder" pn="Block builder" type="bool" v="false" values="false,true" help=""/> -->
      <!--<parameter n="move_mesh_flag" pn="Move mesh" type="bool" v="true" values="false,true" help=""/> -->
      <parameter n="convergence_criterion" pn="Convergence criterion" type="combo" v="Residual_criterion" values ="Residual_criterion,Variable_criterion,And_criterion,Or_criterion" pvalues ="Residual criterion,Variable criterion,And criterion,Or criterion" help="These are the available convergence criteria for the nonlinear iteration">
        <parameter n="variable_relative_tolerance" pn="Variable relative tolerance" type="double" v="1e-4" parent="Variable_criterion,And_criterion,Or_criterion" help="Relative displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="variable_absolute_tolerance" pn="Variable absolute tolerance" type="double" v="1e-9" parent="Variable_criterion,And_criterion,Or_criterion" help="Absolute displacement tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_relative_tolerance" pn="Residual relative tolerance" type="double" v="1e-4" parent="Residual_criterion,And_criterion,Or_criterion" help="Relative residual tolerance employed in the nonlinear iteration"/>
        <parameter n="residual_absolute_tolerance" pn="Residual absolute tolerance" type="double" v="1e-9" parent="Residual_criterion,And_criterion,Or_criterion" help="Absolute residual tolerance employed in the nonlinear iteration"/>
      </parameter>
      <parameter n="max_iteration" pn="Maximum iterations" type="integer" v="10" help="This number will break the iterative loop when reached"/>
    </parameter_list>

    <linearSolvers>
      <linearSolverItem n="linear_solver_settings" pn="Linear Solver" help="Default solver Super LU">
        <filterFeatures>
          <filter field="SolverType" value="Standard"/>
        </filterFeatures>
        <defaults>
          <solver n="Automatic">
          </solver>
        </defaults>
      </linearSolverItem>
    </linearSolvers>

    <schemes>
      <scheme n="Bossak" pn="Bossak" help="Scheme for dynamic problems with high frequency accelerations damping" ProductionReady="ProductionReady">
        <parameter_list>
        </parameter_list>
        <element_filters>
          <filter field="kratos_module" value="SolidMechanicsApplication,PfemSolidMechanicsApplication,ContactMechanicsApplication"/>
        </element_filters>

      </scheme>
      <scheme n="Newmark" pn="Newmark" help="Scheme for dynamic problems" ProductionReady="ProductionReady">
        <parameter_list>
        </parameter_list>
        <element_filters>
          <filter field="kratos_module" value="SolidMechanicsApplication,PfemSolidMechanicsApplication,ContactMechanicsApplication"/>
        </element_filters>
      </scheme>
    </schemes>

  </StrategyItem>
  <StrategyItem n="Explicit" pn="Explicit" SolutionType="Dynamic" EquationType="Monolithic" kratos_module="SolidMechanicsApplication" python_module="solid_mechanics_explicit_dynamic_solver" MinimumKratosVersion="8500" App="Solid" ProductionReady="ProductionReady" help="Default solver Super LU">

    <!-- list of options to be filled -->
    <parameter_list>
      <!--<parameter n="reform_dofs_at_each_step" pn="Reform DOFs at each step" type="bool" v="false" values="false,true" -->
